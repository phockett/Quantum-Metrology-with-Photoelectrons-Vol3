@article{Cherepkov1979,
  title = {Spin Polarisation of Photoelectrons Ejected from Unpolarised Atoms},
  author = {Cherepkov, N A},
  year = {1979},
  journal = {Journal of Physics B: Atomic and Molecular Physics},
  volume = {12},
