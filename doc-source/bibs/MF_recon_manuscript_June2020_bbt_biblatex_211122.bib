@article{ren2016DirectEvidenceTwo,
  title = {Direct Evidence of Two Interatomic Relaxation Mechanisms in Argon Dimers Ionized by Electron Impact},
  author = {Ren, Xueguang and Jabbour Al Maalouf, Elias and Dorn, Alexander and Denifl, Stephan},
